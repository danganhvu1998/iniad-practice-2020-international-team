{
  "name": "shozemi-web-app",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "lint": "eslint --fix ./src",
    "dev": "nodemon --exec babel-node src/server.js",
    "start": "node ./dist/server.js",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down --rmi all",
    "docker:connect": "docker exec -it node-backend /bin/bash",
    "docker:deploy": "git pull origin develop && docker system prune -f && npm run docker:clean && sudo npm run docker:start",
    "docker:deploy:dev": "git pull origin anhvu_develop && docker system prune -f && npm run docker:clean && sudo npm run docker:start",
    "test": "NODE_ENV=local jest --silent",
    "logfulTest": "NODE_ENV=local jest",
    "re-migrate": "cd database && sequelize db:migrate --config '../config/local.json' --env database && sequelize db:seed:all --config '../config/local.json' --env database",
    "re-migrate-dev": "cd database && sequelize db:migrate --config '../config/localDev.json' --env database && sequelize db:seed:all --config '../config/localDev.json' --env database"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@hapi/joi-date": "^1.3.0",
    "@sendgrid/mail": "^7.2.6",
    "aws-sdk": "^2.742.0",
    "axios": "^0.20.0",
    "bcrypt": "^3.0.8",
    "big-json": "^3.1.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "helmet": "^4.1.0",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^1.0.5",
    "i18n": "^0.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "query-string": "^6.13.1",
    "rimraf": "^2.7.1",
    "rotating-file-stream": "^1.4.6",
    "sequelize": "^5.22.3",
    "shelljs": "^0.8.4",
    "ssh2": "^0.8.9",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
